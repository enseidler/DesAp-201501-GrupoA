<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field"
	default-cascade="save-update">
	<class name="ar.edu.desapp.groupa.model.medicalConsultation.MedicalConsultation">
		<id column="id" name="id">
			<generator class="native" />
		</id>
		
 		<one-to-one name="diagnoseDisease" class="ar.edu.desapp.groupa.model.disease.Disease" cascade="save-update" />
  		<one-to-one name="treatment" class="ar.edu.desapp.groupa.model.treatment.Treatment" cascade="save-update" /> 

	</class>
</hibernate-mapping>
