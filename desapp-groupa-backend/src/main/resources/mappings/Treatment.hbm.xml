<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field"
	default-cascade="save-update">
	<class name="ar.edu.desapp.groupa.model.treatment.Treatment">
		<id column="ID" name="id">
			<generator class="native" />
		</id>
		
 		<list name="medicines" lazy="false">
			<key column="medicineID" />
			<list-index column="id" />
			<one-to-many class="ar.edu.desapp.groupa.model.medicine.Medicine" />
		</list>
		
		<list name="medicalPractices" lazy="false">
			<key column="practiceID" />
			<list-index column="id" />
			<one-to-many class="ar.edu.desapp.groupa.model.medicalPractice.MedicalPractice" />
		</list>

	</class>
</hibernate-mapping>
